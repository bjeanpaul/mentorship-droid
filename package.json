    "react": "^15.2.1",
    "react-native": "^0.31.0",
    "react-native-calendar": "^0.7.0",
    "react-redux": "^4.4.5",
    "redux": "^3.4.0",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.1.2",
    "babel-polyfill": "^6.7.4",
    "enzyme": "^2.4.1",
    "eslint": "^2.3.0",
